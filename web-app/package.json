    "author": "David Zukowski",
    "license": "MIT",
    "type": "module",
    "engines": {
        "node": ">=16.7.0"
    },
    "prettier": "@davezuko/prettier-config",
    "eslintConfig": {
        "extends": "@davezuko/eslint-config",
        "ignorePatterns": [
            "dist"
        ]
    },
    "scripts": {
        "build": "node ./scripts/build.js --minify --strip-console",
        "build-dev": "node ./scripts/build.js --devtools --minify=0",
